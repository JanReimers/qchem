=\left[g_{a}^{L}\left(r\right)g_{c}^{L}\left(r\right)+g_{a}^{S}\left(r\right)g_{c}^{S}\left(r\right)\right]\Omega_{\kappa_{a}m_{ja}}^{\dagger}\Omega_{\kappa_{c}m_{jc}}
\]

\end_inset


\begin_inset Formula 
\[
=\left[g_{a}^{L}\left(r\right)g_{c}^{L}\left(r\right)+g_{a}^{S}\left(r\right)g_{c}^{S}\left(r\right)\right]\left(\Omega_{\kappa_{a}m_{ja}}^{\alpha*}\Omega_{\kappa_{c}m_{jc}}^{\alpha}+\Omega_{\kappa_{a}m_{ja}}^{\beta*}\Omega_{\kappa_{c}m_{jc}}^{\beta}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
g_{abcd}=\int d^{3}\overrightarrow{r_{1}}\int d^{3}\overrightarrow{r_{2}}\;g_{a}^{\dagger}\left(\overrightarrow{r_{1}}\right)g_{b}^{\dagger}\left(\overrightarrow{r_{2}}\right)\frac{1}{r_{12}}g_{c}\left(\overrightarrow{r_{1}}\right)g_{d}\left(\overrightarrow{r_{2}}\right)
\]

\end_inset

using 
\begin_inset Formula 
\[
\frac{1}{r_{12}}=\sum_{k=0}^{\infty}\frac{4\pi}{2k+1}\frac{r_{<}^{k}}{r_{>}^{k+1}}\sum_{m=-k}^{k}Y_{km}^{*}\left(\Omega_{1}\right)Y_{km}\left(\Omega_{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
g_{abcd}=\sum_{km}R_{k}\left(abcd\right)A_{k}\left(\kappa_{a},m_{ja},\kappa_{c}m_{jc},-m\right)A_{k}\left(\kappa_{b},m_{jb},\kappa_{d}m_{jd},m\right)
