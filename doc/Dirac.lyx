\]

\end_inset


\begin_inset Formula 
\[
=C_{a}C_{c}\int d\theta d\phi Y_{l_{a}m_{la}}^{*}\left(\theta,\phi\right)Y_{km}\left(\theta,\phi\right)Y_{l_{c}m_{lc}}\left(\theta,\phi\right)
\]

\end_inset


\begin_inset Formula 
\[
=C_{a}C_{c}\left(-1\right)^{m_{c}}\sqrt{\frac{\left(2l_{a}+1\right)\left(2l_{c}+1\right)\left(2k+1\right)}{4\pi}}\left(\begin{array}{ccc}
