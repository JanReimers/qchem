\begin_layout Standard
Since the sign of 
\begin_inset Formula $\kappa$
\end_inset

 determines the 
\begin_inset Formula $+/-$
\end_inset

 sector,
 we can now remove those labels.
 We can then rewrite
\begin_inset Formula 
\[
\Omega_{\kappa m_{j}}=\frac{1}{\sqrt{2\left|\kappa\right|-1}}\begin{bmatrix}\sqrt{\left|\kappa\right|+m_{j}-\nicefrac{1}{2}}Y_{\left|\kappa\right|-1,m_{j}-\nicefrac{1}{2}}\\
\sqrt{\left|\kappa\right|-m_{j}-\nicefrac{1}{2}}Y_{\left|\kappa\right|-1,m_{j}+\nicefrac{1}{2}}
\end{bmatrix},\quad\kappa<0
\]

\end_inset


\begin_inset Formula 
\[
\Omega_{\kappa m_{j}}=\frac{1}{\sqrt{2\kappa+1}}\begin{bmatrix}-\sqrt{\kappa-m_{j}+\nicefrac{1}{2}}Y_{\kappa,m_{j}-\nicefrac{1}{2}}\\
\sqrt{\kappa+m_{j}+\nicefrac{1}{2}}Y_{\kappa,m_{j}+\nicefrac{1}{2}}
\end{bmatrix},\quad\kappa>0
\]

\end_inset

Or in terms of 
\begin_inset Formula $l$
\end_inset


\begin_inset Formula 
\[
\Omega_{\kappa m_{j}}=\frac{1}{\sqrt{2l+1}}\begin{bmatrix}\sqrt{l+m_{l}}Y_{l,m_{l}}\\
\sqrt{l-m_{l}}Y_{l,m_{l}}
\end{bmatrix},\quad\kappa<0
\]

\end_inset


\begin_inset Formula 
\[
\Omega_{\kappa m_{j}}=\frac{1}{\sqrt{2l+1}}\begin{bmatrix}-\sqrt{l-m_{l}}Y_{l,m_{l}}\\
\sqrt{l+m_{l}}Y_{l,m_{l}}
\end{bmatrix},\quad\kappa>0
\]

\end_inset

As expected these spherical spinors obey the usual orthogonality conditions
\begin_inset Formula 
\begin{equation}
\intop_{0}^{\pi}sin\theta d\theta\int_{0}^{2\pi}d\phi\Omega_{\kappa'm_{j}'}^{\dagger}\left(\theta,\phi\right)\Omega_{\kappa m_{j}}\left(\theta,\phi\right)=\delta_{\kappa'\kappa}\delta_{m_{j}'m_{j}}\label{eq:OmegaOrtho}
\end{equation}

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\pi}sin\theta d\theta\int_{0}^{2\pi}d\phi\left[\Omega_{\kappa'm_{j}'}^{\alpha*}\Omega_{\kappa m_{j}}^{\alpha}+\Omega_{\kappa'm_{j}'}^{\beta\dagger}\Omega_{\kappa m_{j}}^{\beta}\right]
\]

\end_inset

The individual terms are also orthogonal,
 but not ortho-normal
\begin_inset Formula 
\[
\intop_{0}^{\pi}sin\theta d\theta\int_{0}^{2\pi}d\phi\Omega_{\kappa'm_{j}'}^{\alpha*}\Omega_{\kappa m_{j}}^{\alpha}=\delta_{\kappa'\kappa}\delta_{m_{j}'m_{j}}\begin{cases}
\frac{\left|\kappa\right|+m_{j}-\nicefrac{1}{2}}{2\left|\kappa\right|-1} & \kappa<0\\
\frac{\kappa-m_{j}+\nicefrac{1}{2}}{2\kappa+1} & \kappa>0
\end{cases}
\]

\end_inset


\begin_inset Formula 
\[
\intop_{0}^{\pi}sin\theta d\theta\int_{0}^{2\pi}d\phi\Omega_{\kappa'm_{j}'}^{\beta*}\Omega_{\kappa m_{j}}^{\beta}=\delta_{\kappa'\kappa}\delta_{m_{j}'m_{j}}\begin{cases}
\frac{\left|\kappa\right|-m_{j}-\nicefrac{1}{2}}{2\left|\kappa\right|-1} & \kappa<0\\
\frac{\kappa+m_{j}+\nicefrac{1}{2}}{2\kappa+1} & \kappa>0
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
Johnson 
\begin_inset CommandInset citation
LatexCommand citet
key "Johnson2007"
literal "false"

\end_inset

 proves these two identities which will be used for isolating the radial wave equations:
\begin_inset Formula 
\begin{equation}
\boldsymbol{\sigma}\cdot\hat{r}\Omega_{\kappa m}=-\Omega_{-\kappa m}\label{eq:Omega1}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\boldsymbol{\sigma}\cdot\boldsymbol{p}f\left(r\right)\Omega_{\kappa m}=i\left(\frac{\partial f}{\partial r}+\frac{\kappa+1}{r}f\right)\Omega_{-\kappa m}\label{eq:Omega2}
\end{equation}

\end_inset


