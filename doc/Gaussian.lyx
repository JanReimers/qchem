<p_{x}p_{x}\parallel p_{y}p_{y}>=\lambda\:\sum_{N_{ab}=0}^{2}\sum_{L_{cd}=0}^{2}d_{N_{ab}}^{11}e_{L_{cd}}^{11}\:\left(-1\right)^{L_{cd}}R_{N_{ab},L_{cd},0}
\]

\end_inset

As usual only certain combinations 
\begin_inset Formula $\{N_{ab},N_{cd}\}=\{0,0\},\{0,2\},\{2,0\},\{2,2\}$
\end_inset

 result in even 
\begin_inset Formula $N_{ab}\&N_{cd}$
\end_inset

 
\begin_inset Formula 
\[
<p_{x}p_{x}\parallel p_{y}p_{y}>=\lambda\:\left[d_{0}^{11}e_{0}^{11}R_{000}+d_{2}^{11}e_{0}^{11}R_{200}+d_{0}^{11}e_{2}^{11}R_{020}+d_{2}^{11}e_{2}^{11}R_{220}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\lambda\:\left[\frac{1}{2\alpha_{ab}}\cdot\frac{1}{2\alpha_{cd}}\cdot1+\frac{1}{4\alpha_{ab}^{2}}\cdot\frac{1}{2\alpha_{cd}}\cdot\left(-\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)+\frac{1}{2\alpha_{ab}}\cdot\frac{1}{4\alpha_{cd}^{2}}\cdot\left(-\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)+\frac{1}{4\alpha_{ab}^{2}}\cdot\frac{1}{4\alpha_{cd}^{2}}\cdot R_{220}\right]
\]

\end_inset


\begin_inset Formula 
\[
R_{220,0}=1\cdot R_{020,1}=1\cdot1\cdot R_{000,2}=\left(-2\alpha\right)^{2}F_{2}(0)=4\alpha^{2}\frac{1}{5}=\frac{4}{5}\left(\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)^{2}
\]

\end_inset


\begin_inset Formula 
\[
<p_{x}p_{x}\parallel p_{y}p_{y}>=\frac{\lambda}{\alpha_{ab}\alpha_{cd}}\left[\frac{1}{4}-\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\left(\frac{1}{4\alpha_{ab}}\cdot\frac{1}{2}+\frac{1}{2}\cdot\frac{1}{4\alpha_{cd}}\right)+\frac{1}{4\alpha_{ab}}\cdot\frac{1}{4\alpha_{cd}}\frac{3}{5}\left(\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)^{2}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{\alpha_{ab}\alpha_{cd}}\left[\frac{1}{4}-\frac{2}{3\cdot8}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\left(\frac{1}{\alpha_{ab}}+\frac{1}{\alpha_{cd}}\right)+\frac{1}{20}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4\alpha_{ab}\alpha_{cd}}\left[1-\frac{1}{3}+\frac{1}{5}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4\alpha_{ab}\alpha_{cd}}\left[\frac{2}{3}+\frac{1}{5}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
