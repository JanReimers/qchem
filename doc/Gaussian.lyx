<sp_{x}\parallel sp_{xy}>=\lambda\:\sum_{N_{ab}=0}^{1}\sum_{L_{cd}=0}^{1}d_{N_{ab}}^{01}e_{L_{cd}}^{01}\:\left(-1\right)^{L_{cd}}R_{N_{ab},L_{cd},0}
\]

\end_inset

but 
\begin_inset Formula $R_{N_{ab},L_{cd},0}=0$
\end_inset

 unless 
\begin_inset Formula $N_{ab}\&L_{cd}$
\end_inset

 are even which leaves only one term in sum 
\begin_inset Formula $N_{ab}=N_{cd}=0$
\end_inset

 a.
\begin_inset Formula 
\[
<sp_{x}\parallel sp_{x}>=\lambda\:\left[d_{0}^{01}e_{0}^{01}R_{000}\right]=0
\]

\end_inset


\end_layout

\begin_layout Subsection*
Case
\begin_inset Formula $\left\langle ss\parallel p_{x}p_{x}\right\rangle $
\end_inset


\end_layout

\begin_layout Standard
Now 
\begin_inset Formula $n_{c}=n_{d}=1$
\end_inset


\begin_inset Formula 
\[
<ss\parallel p_{x}p_{x}>=\lambda\:\sum_{N_{cd}=0}^{2}d_{N_{cd}}^{11}\:\left(-1\right)^{N_{cd}}R_{N_{cd},0,0}
\]

\end_inset


\begin_inset Formula 
\[
\lambda\:\left[d_{0}^{11}R_{000}+d_{2}^{11}R_{200}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
d_{N}^{11}=\frac{1}{2\alpha_{cd}}d_{N-1}^{01}+\left(N+1\right)d_{N+1}^{01}
\]

\end_inset


\begin_inset Formula 
\[
=\frac{1}{2\alpha_{cd}}d_{N-1}^{10}+\left(N+1\right)d_{N+1}^{10}
\]

\end_inset


\begin_inset Formula 
\[
d_{0}^{11}=\frac{1}{2\alpha_{cd}}d_{-1}^{01}+d_{1}^{01}
\]

\end_inset


\begin_inset Formula 
\[
=0+\frac{1}{2\alpha_{cd}}
\]

\end_inset


\begin_inset Formula 
\[
d_{2}^{11}=\frac{1}{2\alpha_{cd}}d_{1}^{01}+\left(N+1\right)d_{3}^{01}
\]

\end_inset


\begin_inset Formula 
\[
\frac{1}{\left(2\alpha_{cd}\right)^{2}}+0
\]

\end_inset

and finally
\begin_inset Formula 
\[
<ss\parallel p_{x}p_{x}>=\lambda\:\left[\frac{1}{2\alpha_{cd}}\bullet1-\frac{1}{\left(2\alpha_{cd}\right)^{2}}\bullet\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{2\alpha_{cd}}\left[1-\frac{1}{3}\frac{\alpha_{ab}}{\alpha_{abcd}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{2\alpha_{cd}}\left[\frac{3\alpha_{abcd}-\alpha_{ab}}{3\alpha_{abcd}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda\left(2\alpha_{ab}+3\alpha_{cd}\right)}{6\alpha_{cd}\alpha_{abcd}}
\]

\end_inset


\end_layout

