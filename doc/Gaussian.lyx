\begin_layout Section*
Case
\begin_inset Formula $\left\langle p_{x}p_{x}\parallel p_{x}p_{x}\right\rangle $
\end_inset


\end_layout

\begin_layout Standard
Now 
\begin_inset Formula $n_{a}=n_{b}=n_{c}=n_{d}=1$
\end_inset

.
\begin_inset Formula 
\[
<p_{x}p_{x}\parallel p_{x}p_{x}>=\lambda\:\sum_{N_{ab}=0}^{2}\sum_{N_{cd}=0}^{2}d_{N_{ab}}^{11}d_{N_{cd}}^{11}\:\left(-1\right)^{N_{cd}}R_{N_{ab}+N_{cd},0,0}
\]

\end_inset

As usual only certain combinations 
\begin_inset Formula $\{N_{ab},N_{cd}\}=\{0,0\},\{0,2\},\{2,0\},\{1,1\},\{2,2\}$
\end_inset

 result in even 
\begin_inset Formula $N_{ab}+N_{cd}$
\end_inset

 
\begin_inset Formula 
\[
<p_{x}p_{x}\parallel p_{x}p_{x}>=\lambda\:\left[d_{0}^{11}d_{0}^{11}R_{000}+d_{2}^{11}d_{0}^{11}R_{200}+d_{0}^{11}d_{2}^{11}R_{200}-d_{1}^{11}d_{1}^{11}R_{200}+d_{2}^{11}d_{2}^{11}R_{400}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\lambda\:\left[\frac{1}{2\alpha_{ab}}\cdot\frac{1}{2\alpha_{cd}}\cdot1+\frac{1}{4\alpha_{ab}^{2}}\cdot\frac{1}{2\alpha_{cd}}\cdot\left(-\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)+\frac{1}{2\alpha_{ab}}\cdot\frac{1}{4\alpha_{cd}^{2}}\cdot\left(-\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)-0\cdot0\cdot R_{200}+\frac{1}{4\alpha_{ab}^{2}}\cdot\frac{1}{4\alpha_{cd}^{2}}\cdot R_{400}\right]
\]

\end_inset


\begin_inset Formula 
\[
R_{400,0}=3R_{200,1}=3\cdot1\cdot R_{000,2}=3\cdot\left(-2\alpha\right)^{2}F_{2}(0)=12\alpha^{2}\frac{1}{5}=\frac{12}{5}\left(\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)^{2}
\]

\end_inset


\begin_inset Formula 
\[
<p_{x}p_{x}\parallel p_{x}p_{x}>=\frac{\lambda}{\alpha_{ab}\alpha_{cd}}\left[\frac{1}{4}-\frac{2}{3}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\left(\frac{1}{4\alpha_{ab}}\cdot\frac{1}{2}+\frac{1}{2}\cdot\frac{1}{4\alpha_{cd}}\right)+\frac{1}{4\alpha_{ab}}\cdot\frac{1}{4\alpha_{cd}}\frac{12}{5}\left(\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\right)^{2}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{\alpha_{ab}\alpha_{cd}}\left[\frac{1}{4}-\frac{2}{3\cdot8}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}}\left(\frac{1}{\alpha_{ab}}+\frac{1}{\alpha_{cd}}\right)+\frac{3}{20}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4\alpha_{ab}\alpha_{cd}}\left[1-\frac{1}{3}+\frac{3}{5}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4\alpha_{ab}\alpha_{cd}}\left[\frac{2}{3}+\frac{3}{5}\frac{\alpha_{ab}\alpha_{cd}}{\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4}\left[\frac{2}{3\alpha_{ab}\alpha_{cd}}+\frac{3}{5\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4}\left[\frac{10\alpha_{abcd}^{2}}{15\alpha_{ab}\alpha_{cd}\alpha_{abcd}^{2}}+\frac{9\alpha_{ab}\alpha_{cd}}{15\alpha_{ab}\alpha_{cd}\alpha_{abcd}^{2}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4}\frac{\left(5\alpha_{ab}+2\alpha_{cd}\right)\left(5\alpha_{ac}+2\alpha_{cd}\right)}{15\alpha_{ab}\alpha_{cd}\alpha_{abcd}^{2}}
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{4}\frac{\left(3\alpha_{ab}+2\alpha_{abcd}\right)\left(3\alpha_{ac}+2\alpha_{abcd}\right)}{15\left(\alpha_{ab}\alpha_{abcd}\right)\left(\alpha_{cd}\alpha_{abcd}\right)}
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\lambda}{60}\left(\frac{3}{\alpha_{abcd}}+\frac{2}{\alpha_{ab}}\right)\left(\frac{3}{\alpha_{abcd}}+\frac{2}{\alpha_{cd}}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection*
