\begin_layout Section*
The Polarized Gaussian basis function
\end_layout

\begin_layout Standard
The standard polarized Gaussian basis function centered at 
\begin_inset Formula \ensuremath{\vec{A}}

\end_inset

 is 
\begin_inset Formula 
\begin{equation}
g(\vec{r})=(r_{x}-A_{x})^{n}(r_{y}-A_{y})^{l}(r_{z}-A_{z})^{m}\:e^{-\alpha(\vec{r}-\vec{A})^{2}}\label{PolarizedGaussian}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
=\left(\frac{1}{2\alpha}\frac{\partial}{\partial A_{x}}\right)^{n}\left(\frac{1}{2\alpha}\frac{\partial}{\partial A_{y}}\right)^{l}\left(\frac{1}{2\alpha}\frac{\partial}{\partial A_{z}}\right)^{m}\:e^{-\alpha(\vec{r}-\vec{A})^{2}}.\label{DifferentialGuassian}
\end{equation}

\end_inset

 The Fourier transform is (using) 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
g(\vec{k})=\left(\frac{1}{2\alpha}\frac{\partial}{\partial A_{x}}\right)^{n}\left(\frac{1}{2\alpha}\frac{\partial}{\partial A_{y}}\right)^{l}\left(\frac{1}{2\alpha}\frac{\partial}{\partial A_{z}}\right)^{m}\:\left(\frac{\pi}{\alpha}\right)^{\frac{3}{2}}\:e^{i\vec{k}\cdot\vec{A}}\:e^{-\frac{k^{2}}{4\alpha}}
\]

\end_inset


\begin_inset Formula 
\begin{equation}
g(\vec{k})=\left(\frac{1}{2\alpha}ik_{x}\right)^{n}\left(\frac{1}{2\alpha}ik_{y}\right)^{l}\left(\frac{1}{2\alpha}ik_{z}\right)^{m}\:\left(\frac{\pi}{\alpha}\right)^{\frac{3}{2}}\:e^{i\vec{k}\cdot\vec{A}}\:e^{-\frac{k^{2}}{4\alpha}}.\label{FTGuassian}
\end{equation}

\end_inset

This is useful for evaluation lattice sums of of integrals such as 
\begin_inset Formula 
\begin{equation}
S_{ab}(\vec{k})=\sum_{\vec{R}}e^{-i\vec{k}\cdot\vec{R}}\:\int d^{3}\vec{r}\:g_{a}(\vec{r})g_{b}(\vec{r}+\vec{R})\label{Blochoverlap}
\end{equation}

\end_inset


\begin_inset Formula 
\[
=\sum_{\vec{R}}e^{-i\vec{k}\cdot\vec{R}}\:\left(\frac{1}{2\pi}\right)^{6}\:\int d^{3}\vec{k}_{1}\:\int d^{3}\vec{k}_{2}\:g_{a}(\vec{k}_{1})g_{b}(\vec{k}_{2})\:e^{-i\vec{k}_{2}\cdot\vec{R}}\:\int d^{3}\vec{r}\:e^{-i(\vec{k}_{1}+\vec{k}_{2})\cdot\vec{r}}
\]

\end_inset


\begin_inset Formula 
\[
=\sum_{\vec{R}}e^{-i\vec{k}\cdot\vec{R}}\:\left(\frac{1}{2\pi}\right)^{6}\:\int d^{3}\vec{k}_{1}\:\int d^{3}\vec{k}_{2}\:g_{a}(\vec{k}_{1})g_{b}(\vec{k}_{2})\:e^{-i\vec{k}_{2}\cdot\vec{R}}\:(2\pi)^{3}\delta(\vec{k}_{1}+\vec{k}_{2})
\]

\end_inset


\begin_inset Formula 
\[
=\left(\frac{1}{2\pi}\right)^{3}\:\int d^{3}\vec{k}_{1}\:g_{a}(\vec{k}_{1})g_{b}(-\vec{k}_{1})\:\sum_{\vec{R}}e^{i(\vec{k}_{1}-\vec{k})\cdot\vec{R}}
\]

\end_inset


\begin_inset Formula 
\[
=\left(\frac{1}{2\pi}\right)^{3}\:\int d^{3}\vec{k}_{1}\:g_{a}(\vec{k}_{1})g_{b}(-\vec{k}_{1})\:\frac{\left(2\pi\right)^{3}}{\Omega}\sum_{\vec{G}}\delta(\vec{G}+\vec{k}_{1}-\vec{k})
\]

\end_inset


\begin_inset Formula 
\begin{equation}
=\frac{1}{\Omega}\:\sum_{\vec{G}}\:g_{a}(\vec{k}-\vec{G})g_{b}(\vec{G}-\vec{k})\label{RLsumforBlochOverlap}
\end{equation}

\end_inset


\end_layout

\begin_layout Section
Slater Functions for Atoms
\end_layout

\begin_layout Standard
Slater function look like
\begin_inset Formula 
\[
g(l_{a},m_{a},\alpha_{a};r,\theta,\phi)=g_{a}(\overrightarrow{r})=r^{n_{a}-1}e^{-\alpha_{a}r}\sqrt{4\pi}Y_{l_{a}m_{a}}(\theta,\phi)
\]

\end_inset

The factor of 
\begin_inset Formula $\sqrt{4\pi}$
\end_inset

 is there to compensate for the fact that the spherical harmonic are normalized,
 and to get an overlap value of 
\begin_inset Formula $\left(\frac{\pi}{\alpha}\right)^{3/2}$
\end_inset

 as found in the literature.
 For one electron integrals the angular part does nothing other than enforce some orthogonality between between basis functions with differing 
\begin_inset Formula $l$
\end_inset

 values,
 and degeneracy between between basis functions with differing 
\begin_inset Formula $m$
\end_inset

 values.
 As a result we end up with a symmetry adapted basis set with separated secular equations for each 
\begin_inset Formula $l$
\end_inset

 value.
 Levels with quantum number 
\begin_inset Formula $l$
\end_inset

 have degeneracy of 
\begin_inset Formula $2l+1$
\end_inset

 for a spin polarized calculation.
\end_layout

\begin_layout Standard
The relation
\begin_inset Formula 
\[
S(n,\alpha)=\int dr\:r^{n}e^{-\alpha r}=\frac{n!}{\alpha^{n+1}}
\]

\end_inset

is used throughout.
 The overlap looks like
\begin_inset Formula 
\[
\int d^{3}r\:g_{a}(\overrightarrow{r})g_{b}(\overrightarrow{r})=\delta_{l_{a}l_{b}}\delta_{m_{a}m_{b}}\int dr\:r^{n_{a}+n_{b}}e^{-\left(\alpha_{a}+\alpha_{b}\right)r}
\]

\end_inset


\begin_inset Formula 
\[
S(n_{a}+n_{b},\alpha_{a}+\alpha_{b})=\frac{\left(n_{a}+n_{b}\right)!}{\left(\alpha_{a}+\alpha_{b}\right)^{n_{a}+n_{b}+1}}=\frac{n_{ab}!}{\alpha_{ab}^{n_{ab}+1}}
\]

\end_inset

which immediately defines the normalization constant.
 The kinetic energy is
\begin_inset Formula 
\[
-\frac{1}{2}\int d^{3}r\:g_{a}(\overrightarrow{r})\nabla^{2}g_{b}(\overrightarrow{r})
\]

\end_inset

which often get reports in asymmetric form.
 To get a symmetric form with split the radial part of into two factors
\begin_inset Formula 
\[
=-\frac{1}{2}\int d^{3}r\:\left[\frac{\partial}{\partial r}rg_{a}(\overrightarrow{r})\right]\left[\frac{\partial}{\partial r}rg_{b}(\overrightarrow{r})\right]
\]

\end_inset


\begin_inset Formula 
\[
=-\frac{1}{2}\int d^{3}r\:\left[\frac{\partial}{\partial r}r^{n_{a}}e^{-\alpha_{a}r}\right]\left[\frac{\partial}{\partial r}r^{n_{b}}e^{-\alpha_{b}r}\right]
\]

\end_inset


\begin_inset Formula 
\[
=-\frac{1}{2}\int d^{3}r\:\left[n_{a}r^{n_{a}-1}e^{-\alpha_{a}r}-r^{n_{a}}\alpha_{a}e^{-\alpha_{a}r}\right]\left[n_{b}r^{n_{b}-1}e^{-\alpha_{b}r}-r^{n_{b}}\alpha_{b}e^{-\alpha_{b}r}\right]
\]

\end_inset


\begin_inset Formula 
\[
-\frac{1}{2}\int d^{3}r\:\left[n_{a}r^{n_{a}-1}-r^{n_{a}}\alpha_{a}\right]\left[n_{b}r^{n_{b}-1}-r^{n_{b}}\alpha_{b}\right]e^{-\alpha_{ab}r}
\]

\end_inset


\begin_inset Formula 
\[
-\frac{1}{2}\int d^{3}r\:\left[n_{a}n_{b}r^{n_{ab}-2}-\left(n_{a}\alpha_{b}+n_{b}\alpha_{a}\right)r^{n_{ab}-1}+r^{n_{ab}}\alpha_{a}\alpha_{b}\right]e^{-\alpha_{ab}r}
\]

\end_inset


\begin_inset Formula 
\[
=-\frac{1}{2}\left[n_{a}n_{b}S\left(n-2,\alpha\right)-\left(n_{a}\alpha_{b}+n_{b}\alpha_{a}\right)S\left(n-1,\alpha\right)+\alpha_{a}\alpha_{b}S\left(n,\alpha\right)\right]
\]

\end_inset

Adding the angular contribution we get:
\begin_inset Formula 
\[
-\frac{1}{2}\left[\left[\left(n_{a}-1\right)\left(n_{b}-1\right)-l(l+1)\right]S\left(n-2,\alpha\right)-\left(\alpha_{a}n_{b}+\alpha_{b}n_{a}\right)S\left(n-1,\alpha\right)+\alpha_{a}\alpha_{b}S\left(n,\alpha\right)\right]
\]

\end_inset

and the nuclear attraction is
\begin_inset Formula 
\[
\int d^{3}r\:g_{a}(\overrightarrow{r})\frac{-Z}{r}g_{b}(\overrightarrow{r})=-Z\frac{\left(n-1\right)!}{\alpha^{n}}
\]

\end_inset

The two electron integral can be factored into a sum over angular (
\begin_inset Formula $r_{k}$
\end_inset

) and radial (
\begin_inset Formula $R^{k}$
\end_inset

) parts.
\begin_inset Formula 
\[
\int d^{3}r\:g_{a}(\overrightarrow{r_{1}})g_{b}(\overrightarrow{r_{1}})\frac{1}{r_{12}}g_{c}(\overrightarrow{r_{2}})g_{d}(\overrightarrow{r_{2}})=\sum_{k}r_{k}\left(l_{a},l_{b},l_{c},l_{d}\right)R^{k}\left(a,b,c,d\right)
\]

\end_inset

where
\begin_inset Formula 
\[
r_{k}\left(l_{a},l_{b},l_{c},l_{d}\right)=\left(-1\right)^{l_{a}+l_{b}+L}\sqrt{\left(2l_{a}+1\right)\left(2l_{b}+1\right)\left(2l_{c}+1\right)\left(2l_{d}+1\right)}
\]

\end_inset


\begin_inset Formula 
\[
\times\left(\begin{array}{ccc}
l_{a} & k & l_{c}\\
0 & 0 & 0
\end{array}\right)\left(\begin{array}{ccc}
l_{b} & k & l_{d}\\
0 & 0 & 0
\end{array}\right)\left(\begin{array}{ccc}
l_{a} & l_{b} & L\\
l_{d} & l_{c} & k
\end{array}\right)
\]

\end_inset

and the radial part is
\begin_inset Formula 
\[
R^{k}\left(a,b,c,d\right)=\frac{\left(n_{ac}-k-1\right)!}{\alpha_{ac}^{n_{ac}-k}}H_{abcd}^{k}+\frac{\left(n_{bd}-k-1\right)!}{\alpha_{bd}^{n_{bd}-k}}H_{badc}^{k}
\]

\end_inset

where
\begin_inset Formula 
\[
H_{abcd}^{k}=\sum_{\nu=0}^{n_{ac}-k-1}\frac{\left(n_{bd}+k+\nu\right)!\alpha_{ac}^{\nu}}{\alpha_{abcd}^{n_{bd}+k+\nu+1}\nu!}
\]

\end_inset


\begin_inset Formula 
\[
H_{badc}^{k}=\sum_{\nu=0}^{n_{ac}-k-1}\frac{\left(n_{ac}+k+\nu\right)!\alpha_{bd}^{\nu}}{\alpha_{abcd}^{n_{ac}+k+\nu+1}\nu!}
\]

\end_inset


\end_layout

\begin_layout Standard
However these are a little to general,
 because for Coulomb repulsion integrals we have 
\begin_inset Formula $l_{a}=l_{b}$
\end_inset

 and 
\begin_inset Formula $l_{c}=l_{d}.$
\end_inset

 For the exchange case 
\begin_inset Formula $l_{a}=l_{c}$
\end_inset

 and 
\begin_inset Formula $l_{b}=l_{d}.$
\end_inset

 Also the angular integrals enforce:
\begin_inset Formula 
\[
\left|l_{a}-l_{b}\right|\leq k\leq l_{a}+l_{b}
\]

\end_inset


\begin_inset Formula 
\[
\left|l_{c}-l_{d}\right|\leq k\leq l_{c}+l_{d}
\]

\end_inset


\begin_inset Formula 
\[
k\;even
\]

\end_inset


\end_layout

\begin_layout Subsection
Repulsion expressions
\end_layout

\begin_layout Standard
\begin_inset Formula $l_{a}=l_{b}$
\end_inset

 and 
\begin_inset Formula $l_{c}=l_{d},$
\end_inset

 so that 
\begin_inset Formula $n_{ac}=n_{bd}=n$
\end_inset

.
 Also the Wigner 3j symbol is zero unless 
\begin_inset Formula $k=0;$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r_{o}\left(l_{a},l_{c}\right)=\left(-1\right)^{2l_{a}+1+L}\left(2l_{a}+1\right)\left(2l_{c}+1\right)
\]

\end_inset


\begin_inset Formula 
\[
H_{abcd}^{0}=\sum_{\nu=0}^{n-1}\frac{\left(n+\nu\right)!\alpha_{ac}^{\nu}}{\alpha_{abcd}^{n+\nu+1}\nu!}
\]

\end_inset


\begin_inset Formula 
\[
H_{badc}^{0}=\sum_{\nu=0}^{n-1}\frac{\left(n+\nu\right)!\alpha_{bd}^{\nu}}{\alpha_{abcd}^{n+\nu+1}\nu!}
\]

\end_inset


\begin_inset Formula 
\[
R^{0}\left(a,b,c,d\right)=\frac{\left(n-1\right)!}{\alpha_{ac}^{n}}H_{abcd}^{0}+\frac{\left(n-1\right)!}{\alpha_{bd}^{n}}H_{badc}^{0}
\]

\end_inset

Let's see what 
\begin_inset Formula $R^{0}\left(0,0,0,0\right)$
\end_inset

 looks like.
 
\begin_inset Formula $n_{a}=n_{b}=n_{c}=n_{d}=1,$
\end_inset

so 
\begin_inset Formula $n_{ac}=n_{bd}=n=2.$
\end_inset


\begin_inset Formula 
\[
H_{abcd}^{0}=\sum_{\nu=0}^{1}\frac{\left(2+\nu\right)!\alpha_{ac}^{\nu}}{\alpha_{abcd}^{3+\nu}\nu!}=\frac{\left(2\right)!}{\alpha_{abcd}^{3}}+\frac{\left(3\right)!\alpha_{ac}}{\alpha_{abcd}^{4}}=\frac{2}{\alpha_{abcd}^{3}}\left(1+\frac{3\alpha_{ac}}{\alpha_{abcd}}\right)
\]

\end_inset


\begin_inset Formula 
\[
H_{badc}^{0}=\frac{2}{\alpha_{abcd}^{3}}\left(1+\frac{3\alpha_{bd}}{\alpha_{abcd}}\right)
\]

\end_inset


\begin_inset Formula 
\[
R^{0}\left(0,0,0,0\right)=\frac{1}{\alpha_{ac}^{2}}\left[\frac{2}{\alpha_{abcd}^{3}}\left(1+\frac{3\alpha_{ac}}{\alpha_{abcd}}\right)\right]+\frac{1}{\alpha_{bd}^{2}}\left[\frac{2}{\alpha_{abcd}^{3}}\left(1+\frac{3\alpha_{bd}}{\alpha_{abcd}}\right)\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{abcd}^{3}}\left[\frac{1}{\alpha_{ac}^{2}}+\frac{3}{\alpha_{ac}\alpha_{abcd}}+\frac{1}{\alpha_{bd}^{2}}+\frac{3}{\alpha_{bd}\alpha_{abcd}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{abcd}^{3}}\left[\frac{1}{\alpha_{ac}^{2}}+\frac{1}{\alpha_{bd}^{2}}+\frac{3}{\alpha_{abcd}}\left(\frac{1}{\alpha_{ac}}+\frac{1}{\alpha_{bd}}\right)\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{abcd}^{3}}\left[\frac{1}{\alpha_{ac}^{2}}+\frac{1}{\alpha_{bd}^{2}}+\frac{3}{\alpha_{abcd}}\left(\frac{\alpha_{abcd}}{\alpha_{ac}\alpha_{bd}}\right)\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{abcd}^{3}}\left[\frac{1}{\alpha_{ac}^{2}}+\frac{1}{\alpha_{bd}^{2}}+\frac{3}{\alpha_{ac}\alpha_{bd}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{ac}^{2}\alpha_{bd}^{2}\alpha_{abcd}^{3}}\left[\alpha_{bd}^{2}+\alpha_{ac}^{2}+3\alpha_{ac}\alpha_{bd}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{ac}^{2}\alpha_{bd}^{2}\alpha_{abcd}^{3}}\left[\alpha_{abcd}^{2}+\alpha_{ac}\alpha_{bd}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha_{ac}\alpha_{bd}\alpha_{abcd}}\left[\frac{1}{\alpha_{ac}\alpha_{bd}}+\frac{1}{\alpha_{abcd}^{2}}\right]
\]

\end_inset

Recognizing that 
\begin_inset Formula $\alpha_{ac}\alpha_{bd}=\alpha_{ab}\alpha_{cd}$
\end_inset

 we finally get
\begin_inset Formula 
\[
R^{0}\left(0,0,0,0\right)=\frac{2}{\alpha_{ab}\alpha_{cd}\alpha_{abcd}}\left[\frac{1}{\alpha_{ab}\alpha_{cd}}+\frac{1}{\alpha_{abcd}^{2}}\right]
\]

\end_inset

which is the correct result.
\end_layout

