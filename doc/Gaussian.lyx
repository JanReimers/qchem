Multicenter (Molecuale) Polarized Gaussians
\end_layout

\begin_layout Subsection*
Molecular case
\end_layout

\begin_layout Standard
McMurchie and Davidson Scheme M&D express the repulsion integral as
\begin_inset Formula 
\[
<g_{a}g_{b}\Vert g_{c}g_{d}>=\lambda E_{ab}E_{cd}\:\sum_{N_{ab}=0}^{n_{a}+n_{b}}\sum_{N_{cd}=0}^{n_{c}+n_{d}}d_{N_{ab}}^{n_{a}n_{b}}d_{N_{cd}}^{n_{c}n_{d}}\:\sum_{L_{ab}=0}^{l_{a}+l_{b}}\sum_{L_{cd}=0}^{l_{c}+l_{d}}e_{L_{ab}}^{l_{a}l_{b}}e_{L_{cd}}^{l_{c}l_{d}}\:\sum_{M_{ab}=0}^{m_{a}+m_{b}}\sum_{M_{cd}=0}^{m_{c}+m_{d}}f_{M_{ab}}^{m_{a}m_{b}}f_{M_{cd}}^{m_{c}m_{d}}
\]

\end_inset


\begin_inset Formula 
\[
\:\left(-1\right)^{N_{cd}+L_{cd}+M_{cd}}R_{N_{ab}+N_{cd},L_{ab}+l_{cd},M_{ab}+M_{cd}}
\]

\end_inset

where
\begin_inset Formula 
\[
\lambda=\frac{2\pi^{5/2}}{\alpha_{ab}\alpha_{cd}\sqrt{\alpha_{abcd}}}
\]

\end_inset


\begin_inset Formula 
\[
E_{ab}=\exp\left(-\frac{\alpha_{a}\alpha_{b}\left|\overrightarrow{A}-\overrightarrow{B}\right|^{2}}{\alpha_{a}+\alpha_{b}}\right)
\]

\end_inset


\begin_inset Formula 
\[
R_{NLM}=\left(\frac{\partial}{P_{x}}\right)^{N}\left(\frac{\partial}{P_{y}}\right)^{L}\left(\frac{\partial}{P_{z}}\right)^{M}F_{o}(T)
\]

\end_inset


\begin_inset Formula 
\[
T=\frac{\alpha_{ab}\alpha_{ac}}{\alpha_{abcd}}\left|\frac{\alpha_{a}\overrightarrow{A}-\alpha_{b}\overrightarrow{B}}{\alpha_{ab}}-\frac{\alpha_{c}\overrightarrow{C}-\alpha_{d}\overrightarrow{D}}{\alpha_{cd}}\right|^{2}
\]

\end_inset


\begin_inset Formula 
\[
F_{o}\left(T\right)=\intop_{0}^{1}e^{-Tu^{2}}du
\]

\end_inset

is the Boys integral.
 The Hermite expansion coefficients 
\begin_inset Formula $d_{N}^{n_{a},n_{b}}$
\end_inset

obey the recursion relations
\begin_inset Formula 
\[
d_{N}^{n_{a}+1,n_{b}}=\frac{1}{2\alpha_{ab}}d_{N-1}^{n_{a},n_{b}}+\left|\frac{\alpha_{a}\overrightarrow{A}-\alpha_{b}\overrightarrow{B}}{\alpha_{ab}}-\overrightarrow{A}\right|d_{N}^{n_{a},n_{b}}+\left(N+1\right)d_{N+1}^{n_{a},n_{b}}
\]

\end_inset


\begin_inset Formula 
\[
d_{N}^{n_{a},n_{b}+1}=\frac{1}{2\alpha_{ab}}d_{N-1}^{n_{a},n_{b}}+\left|\frac{\alpha_{a}\overrightarrow{A}-\alpha_{b}\overrightarrow{B}}{\alpha_{ab}}-\overrightarrow{B}\right|d_{N}^{n_{a},n_{b}}+\left(N+1\right)d_{N+1}^{n_{a},n_{b}}
\]

\end_inset


\begin_inset Formula 
\[
d_{0}^{00}=1
\]

\end_inset

 and any negative indices yield zero.
\end_layout

\begin_layout Subsection*
Atomic Case
\end_layout

\begin_layout Standard
For a single center we see significant simplications
\begin_inset Formula 
\[
E_{ab}=1,\;T=0\;F_{j}(0)=\frac{1}{2j+1}
\]

\end_inset


\begin_inset Formula 
\[
d_{N}^{n_{a}+1,n_{b}}=d_{N}^{n_{a},n_{b}+1}=\frac{1}{2\alpha_{ab}}d_{N-1}^{n_{a},n_{b}}+\left(N+1\right)d_{N+1}^{n_{a},n_{b}}
\]

\end_inset


\begin_inset Formula 
\[
d_{1}^{n_{a}+1,n_{b}}=d_{1}^{n_{a},n_{b}+1}=\frac{1}{2\alpha_{ab}}d_{0}^{n_{a},n_{b}}+2d_{2}^{n_{a},n_{b}}
\]

\end_inset


\begin_inset Formula 
\[
d_{1}^{10}=d_{1}^{01}=\frac{1}{2\alpha_{ab}}d_{0}^{00}=\frac{1}{2\alpha_{ab}}
\]

\end_inset


\begin_inset Formula 
\[
d_{0}^{20}=d_{0}^{02}=d_{0}^{11}=d_{1}^{10}=d_{1}^{01}=\frac{1}{2\alpha_{ab}}
\]

\end_inset


\begin_inset Formula 
\[
d_{2}^{20}=d_{2}^{02}=d_{2}^{11}=\frac{1}{2\alpha_{ab}}d_{1}^{10}=\frac{1}{\left(2\alpha_{ab}\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
d_{1}^{12}=d_{1}^{12}=\frac{1}{2\alpha_{ab}}d_{0}^{11}+2d_{2}^{11}=\frac{1}{\left(2\alpha_{ab}\right)^{2}}+2\frac{1}{\left(2\alpha_{ab}\right)^{2}}=\frac{3}{\left(2\alpha_{ab}\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
d_{1}^{20}=d_{1}^{02}=d_{1}^{11}=\frac{1}{2\alpha_{ab}}d_{0}^{10}=0
\]

\end_inset

