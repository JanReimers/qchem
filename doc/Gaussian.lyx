\begin_layout Standard
The general Slater integral is
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R_{k}(l_{a},l_{b},l_{c},l_{d})=\iint\frac{r_{<}^{k}}{r_{>}^{k+1}}\:r_{1}^{l_{a}+l_{b}+2}e^{-\alpha_{ab}r_{1}^{2}}\:r_{2}^{l_{c}+l_{d}+2}e^{-\alpha_{cd}r_{2}^{2}}dr_{1}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{l_{a}+l_{b}+2}e^{-\alpha_{ab}r_{1}^{2}}\left(\intop_{0}^{r1}\frac{r_{2}^{k}}{r_{1}^{k+1}}\:r_{2}^{l_{c}+l_{d}+2}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+\intop_{r_{1}}^{\infty}\frac{r_{1}^{k}}{r_{2}^{k+1}}\:r_{2}^{l_{c}+l_{d}+2}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{l_{a}+l_{b}+2}e^{-\alpha_{ab}r_{1}^{2}}\left(\frac{1}{r_{1}^{k+1}}\intop_{0}^{r1}\:r_{2}^{l_{c}+l_{d}+k+2}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+r_{1}^{k}\intop_{r_{1}}^{\infty}\:r_{2}^{l_{c}+l_{d}-k+1}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{l_{a}+l_{b}-k+1}e^{-\alpha_{ab}r_{1}^{2}}\intop_{0}^{r1}\:r_{2}^{l_{c}+l_{d}+k+2}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+\intop_{0}^{\infty}dr_{1}r_{1}^{l_{a}+l_{b}+k+2}e^{-\alpha_{ab}r_{1}^{2}}\intop_{r_{1}}^{\infty}\:r_{2}^{l_{c}+l_{d}-k+1}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}.
\]

\end_inset

We know from the angular rules that 
\begin_inset Formula $l_{a}+l_{b}+k$
\end_inset

 is even,
 in which case 
\begin_inset Formula $l_{a}+l_{b}-k$
\end_inset

 must also be even.
 Defining 
\begin_inset Formula 
\[
L_{+}^{ab}=\frac{1}{2}\left(l_{a}+l_{b}+k+2\right)
\]

\end_inset


\begin_inset Formula 
\[
L_{-}^{ab}=\frac{1}{2}\left(l_{a}+l_{b}-k\right)
\]

\end_inset

and the differential operators
\begin_inset Formula 
\[
D_{ab}^{n}=\left(-\frac{\partial}{\partial\alpha_{ab}}\right)^{n}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R_{k}(l_{a},l_{b},l_{c},l_{d})=\intop_{0}^{\infty}dr_{1}r_{1}^{L_{-}^{ab}+1}e^{-\alpha_{ab}r_{1}^{2}}\intop_{0}^{r1}\:r_{2}^{L_{+}^{cd}}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+\intop_{0}^{\infty}dr_{1}r_{1}^{L_{+}^{ab}}e^{-\alpha_{ab}r_{1}^{2}}\intop_{r_{1}}^{\infty}\:r_{2}^{L_{-}^{cd}+1}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
=D_{ab}^{L_{-}}\intop_{0}^{\infty}dr_{1}r_{1}e^{-\alpha_{ab}r_{1}^{2}}D_{cd}^{L_{+}}\intop_{0}^{r1}\:e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+D_{ab}^{L_{+}}\intop_{0}^{\infty}dr_{1}e^{-\alpha_{ab}r_{1}^{2}}D_{cd}^{L_{-}}\intop_{r_{1}}^{\infty}\:r_{2}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
=D_{ab}^{L_{-}}D_{cd}^{L_{+}}\intop_{0}^{\infty}dr_{1}r_{1}e^{-\alpha_{ab}r_{1}^{2}}\intop_{0}^{r1}\:e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+D_{ab}^{L_{+}}D_{cd}^{L_{-}}\intop_{0}^{\infty}dr_{1}e^{-\alpha_{ab}r_{1}^{2}}\intop_{r_{1}}^{\infty}\:r_{2}e^{-\alpha_{cd}r_{2}^{2}}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
=D_{ab}^{L_{-}}D_{cd}^{L_{+}}\intop_{0}^{\infty}dr_{1}r_{1}e^{-\alpha_{ab}r_{1}^{2}}\intop_{0}^{r1}\:e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+D_{ab}^{L_{+}}D_{cd}^{L_{-}}\frac{1}{2\alpha_{cd}}\intop_{0}^{\infty}dr_{1}e^{-\alpha_{ab}r_{1}^{2}}\left[-e^{-\alpha_{cd}r_{2}^{2}}\right]_{r_{1}}^{\infty}
\]

\end_inset


\begin_inset Formula 
\[
=D_{ab}^{L_{-}}D_{cd}^{L_{+}}\intop_{0}^{\infty}dr_{1}r_{1}e^{-\alpha_{ab}r_{1}^{2}}\intop_{0}^{r1}\:e^{-\alpha_{cd}r_{2}^{2}}dr_{2}+D_{ab}^{L_{+}}D_{cd}^{L_{-}}\frac{1}{2\alpha_{cd}}\intop_{0}^{\infty}dr_{1}e^{-\alpha_{abcd}r_{1}^{2}}
\]

\end_inset


\begin_inset Formula 
\[
=D_{ab}^{L_{-}}D_{cd}^{L_{+}}\frac{1}{2\alpha_{ab}}\frac{1}{4}\frac{\sqrt{\pi}}{\sqrt{\alpha_{abcd}^{3}}}+D_{ab}^{L_{+}}D_{cd}^{L_{-}}\frac{1}{4\alpha_{cd}}\sqrt{\frac{\pi}{\alpha_{abcd}}}
\]

\end_inset


\begin_inset Formula 
\[
=\frac{\sqrt{\pi}}{4}\left[D_{ab}^{\frac{1}{2}\left(l_{a}+l_{b}-l\right)}D_{cd}^{\frac{1}{2}\left(l_{c}+l_{d}+l+2\right)}\frac{1}{\alpha_{ab}\sqrt{\alpha_{abcd}}}+D_{ab}^{\frac{1}{2}\left(l_{a}+l_{b}+l+2\right)}D_{cd}^{\frac{1}{2}\left(l_{c}+l_{d}-l\right)}\frac{1}{\alpha_{cd}\sqrt{\alpha_{abcd}}}\right]
\]

\end_inset

The 
\begin_inset Formula $L^{+}$
\end_inset

 derivatives are easy to evaluate in the general case,
 yielding
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
=\frac{\sqrt{\pi}}{8}\left[\frac{\left[l_{c}+l_{d}+l+1\right]!!}{2^{\frac{1}{2}\left(l_{c}+l_{d}+l\right)}}D_{ab}^{\frac{1}{2}\left(l_{a}+l_{b}-l\right)}\frac{1}{\alpha_{ab}\sqrt{\alpha_{abcd}^{3+l_{c}+l_{d}+l}}}+\frac{\left[l_{a}+l_{b}+l+1\right]!!}{2^{\frac{1}{2}\left(l_{a}+l_{b}+l\right)}}D_{cd}^{\frac{1}{2}\left(l_{c}+l_{d}-l\right)}\frac{1}{\alpha_{cd}\sqrt{\alpha_{abcd}^{3+l_{a}+l_{b}+l}}}\right].
\]

\end_inset

If we define
\begin_inset Formula 
\[
I_{ab}(n)=\frac{1}{\alpha_{ab}\sqrt{\alpha_{abcd}^{n}}}
\]

\end_inset

We can now use this relation recursively to generate the final derivatives
