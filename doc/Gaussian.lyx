\begin_layout Subsection
Some Slater Radial Integrals
\end_layout

\begin_layout Standard
We will need:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\intop dr\:re^{-\alpha r}=\frac{-1}{\alpha}re^{-\alpha r}+\frac{1}{\alpha}\intop dr\:e^{-\alpha r}
\]

\end_inset


\begin_inset Formula 
\[
=-\left[\frac{1}{\alpha}r+\frac{1}{\alpha^{2}}\right]\:e^{-\alpha r}
\]

\end_inset

and
\begin_inset Formula 
\[
\intop dr\:r^{2}e^{-\alpha r}=\frac{-1}{\alpha}r^{2}e^{-\alpha r}+\frac{2}{\alpha}\intop dr\:re^{-\alpha r}
\]

\end_inset


\begin_inset Formula 
\[
=-\left[\frac{1}{\alpha}r^{2}+\frac{2}{\alpha^{2}}r+\frac{2}{\alpha^{3}}\right]e^{-\alpha r}
\]

\end_inset


\end_layout

\begin_layout Standard
Method 1:
 Integration by parts:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
R^{0}\left(0,0,0,0\right)=\iint\frac{1}{r_{>}}r_{1}^{2}e^{-\alpha r_{1}}\:r_{2}^{2}e^{-\beta r_{2}}dr_{1}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{2}e^{-\alpha r_{1}}\left(\frac{1}{r_{1}}\intop_{0}^{r1}\:r_{2}^{2}e^{-\beta r_{2}}dr_{2}+\intop_{r_{1}}^{\infty}\frac{1}{r_{2}}\:r_{2}^{2}e^{-\beta r_{2}}dr_{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{2}e^{-\alpha r_{1}}\left(\frac{1}{r_{1}}\intop_{0}^{r1}\:r_{2}^{2}e^{-\beta r_{2}}dr_{2}+\intop_{r_{1}}^{\infty}r_{2}e^{-\beta r_{2}}dr_{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{2}e^{-\alpha r_{1}}\left(\frac{1}{r_{1}}\left[-\left(\frac{1}{\beta}r^{2}+\frac{2}{\beta^{2}}r+\frac{2}{\beta^{3}}\right)e^{-\beta r}\right]_{o}^{r_{1}}+\left[-\left(\frac{1}{\beta}r+\frac{1}{\beta^{2}}\right)\:e^{-\beta r}\right]_{r_{1}}^{\infty}\right)
\]

\end_inset


\begin_inset Formula 
\[
=\intop_{0}^{\infty}dr_{1}r_{1}^{2}e^{-\alpha r_{1}}\left(\frac{1}{r_{1}}\left[\frac{2}{\beta^{3}}-\left(\frac{1}{\beta}r_{1}^{2}+\frac{2}{\beta^{2}}r_{1}+\frac{2}{\beta^{3}}\right)e^{-\beta r_{1}}\right]+\left[\left(\frac{1}{\beta}r_{1}+\frac{1}{\beta^{2}}\right)\:e^{-\beta r_{1}}\right]\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{3}}\intop_{0}^{\infty}dr_{1}r_{1}e^{-\alpha r_{1}}+\intop_{0}^{\infty}dr_{1}e^{-\left(\alpha+\beta\right)r_{1}}\left(-r_{1}\left(\frac{1}{\beta}r_{1}^{2}+\frac{2}{\beta^{2}}r_{1}+\frac{2}{\beta^{3}}\right)+r_{1}^{2}\left(\frac{1}{\beta}r_{1}+\frac{1}{\beta^{2}}\right)\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{3}\alpha^{2}}+\intop_{0}^{\infty}dr_{1}e^{-\left(\alpha+\beta\right)r_{1}}\left(\frac{1}{\beta}r_{1}^{3}+\frac{r_{1}^{2}}{\beta^{2}}-\left(\frac{1}{\beta}r_{1}^{3}+\frac{2}{\beta^{2}}r_{1}^{2}+\frac{2}{\beta^{3}}r_{1}\right)\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{3}\alpha^{2}}-\intop_{0}^{\infty}dr_{1}e^{-\left(\alpha+\beta\right)r_{1}}\left(\frac{1}{\beta^{2}}r_{1}^{2}+\frac{2}{\beta^{3}}r_{1}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{3}\alpha^{2}}-\frac{2}{\beta^{2}\left(\alpha+\beta\right)^{3}}-\frac{2}{\beta^{3}\left(\alpha+\beta\right)^{2}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{3}\alpha^{2}\left(\alpha+\beta\right)^{3}}\left(\left(\alpha+\beta\right)^{3}-\alpha^{2}\beta-\alpha^{2}\left(\alpha+\beta\right)\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{3}\alpha^{2}\left(\alpha+\beta\right)^{3}}\left(\left(\alpha+\beta\right)\left(\alpha^{2}+2\alpha\beta+\beta^{2}-\alpha^{2}\right)-\alpha^{2}\beta\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{2}\alpha^{2}\left(\alpha+\beta\right)^{3}}\left(\left(\alpha+\beta\right)\left(2\alpha+\beta\right)-\alpha^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{2}\alpha^{2}\left(\alpha+\beta\right)^{3}}\left(2\alpha^{2}+3\alpha\beta+\beta^{2}-\alpha^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{2}\alpha^{2}\left(\alpha+\beta\right)^{3}}\left(\alpha^{2}+3\alpha\beta+\beta^{2}\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\beta^{2}\alpha^{2}\left(\alpha+\beta\right)^{3}}\left(\left(\alpha+\beta\right)^{2}+\alpha\beta\right)
\]

\end_inset


\begin_inset Formula 
\[
\frac{2}{\alpha\beta\left(\alpha+\beta\right)}\left[\frac{1}{\alpha\beta}+\frac{1}{\left(\alpha+\beta\right)^{2}}\right]
\]

\end_inset


\end_layout

\begin_layout Standard
For 
\begin_inset Formula $k>0$
\end_inset


\begin_inset Formula 
\[
R_{k}(l_{a},l_{b},l_{c},l_{d})=\iint\frac{r_{<}^{k}}{r_{>}^{k+1}}\:r_{1}^{l_{a}+l_{b}+2}e^{-\alpha r_{1}}\:r_{2}^{l_{c}+l_{d}+2}e^{-\beta r_{2}}dr_{1}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
\intop_{0}^{\infty}dr_{1}r_{1}^{l_{a}+l_{b}-k+1}e^{-\alpha r_{1}}\intop_{0}^{r1}\:r_{2}^{l_{c}+l_{d}+k+2}e^{-\beta r_{2}}dr_{2}+\intop_{0}^{\infty}dr_{1}r_{1}^{l_{a}+l_{b}+k+2}e^{-\alpha r_{1}^{2}}\intop_{r_{1}}^{\infty}\:r_{2}^{l_{c}+l_{d}-k+1}e^{-\beta r_{2}^{2}}dr_{2}
\]

\end_inset

Defining 
\begin_inset Formula 
\[
L_{+}^{ab}=l_{a}+l_{b}+k+2
\]

\end_inset


\begin_inset Formula 
\[
L_{-}^{ab}=l_{a}+l_{b}-k+1
\]

\end_inset

 
\begin_inset Formula 
\[
R_{k}(l_{a},l_{b},l_{c},l_{d})=\intop_{0}^{\infty}dr_{1}r_{1}^{L_{-}^{ab}}e^{-\alpha r_{1}}\intop_{0}^{r1}\:r_{2}^{L_{+}^{cd}}e^{-\beta r_{2}}dr_{2}+\intop_{0}^{\infty}dr_{1}r_{1}^{L_{+}^{ab}}e^{-\alpha r_{1}^{2}}\intop_{r_{1}}^{\infty}\:r_{2}^{L_{-}^{cd}}e^{-\beta r_{2}^{2}}dr_{2}
\]

\end_inset

and the differential operators
\begin_inset Formula 
\[
D_{\alpha}^{n}=\left(-\frac{\partial}{\partial\alpha}\right)^{n}
\]

\end_inset

we have
\begin_inset Formula 
\[
R_{k}(l_{a},l_{b},l_{c},l_{d})=D_{\alpha}^{L_{-}^{ab}}D_{\beta}^{L_{+}^{cd}}\intop_{0}^{\infty}dr_{1}e^{-\alpha r_{1}}\intop_{0}^{r1}\:e^{-\beta r_{2}}dr_{2}+D_{\alpha}^{L_{+}^{ab}}D_{\beta}^{L_{-}^{cd}}\intop_{0}^{\infty}dr_{1}e^{-\alpha r_{1}}\intop_{r_{1}}^{\infty}\:e^{-\beta r_{2}}dr_{2}
\]

\end_inset


\begin_inset Formula 
\[
\intop_{0}^{\infty}dr_{1}e^{-\alpha r_{1}}\intop_{0}^{r1}\:e^{-\beta r_{2}}dr_{2}=\intop_{0}^{\infty}dr_{1}e^{-\alpha r_{1}}\frac{1}{\beta}\left[1-e^{-\beta r_{1}}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{1}{\beta}\left[\frac{1}{\alpha}-\frac{1}{\alpha+\beta}\right]=\frac{1}{\beta}\left[\frac{\alpha+\beta}{\alpha\left(\alpha+\beta\right)}-\frac{\alpha}{\alpha\left(\alpha+\beta\right)}\right]
\]

\end_inset


\begin_inset Formula 
\[
=\frac{1}{\alpha\left(\alpha+\beta\right)}
\]

\end_inset


\begin_inset Formula 
\[
\intop_{0}^{\infty}dr_{1}e^{-\alpha r_{1}}\intop_{r_{1}}^{\infty}\:e^{-\beta r_{2}}=\intop_{0}^{\infty}dr_{1}e^{-\alpha r_{1}}\frac{1}{\beta}e^{-\beta r_{1}}
\]

\end_inset


\begin_inset Formula 
\[
\frac{1}{\beta\left(\alpha+\beta\right)}
\]

\end_inset


\end_layout

\begin_layout Standard
So now
\begin_inset Formula 
\[
R_{k}(l_{a},l_{b},l_{c},l_{d})=D_{\alpha}^{L_{-}^{ab}}D_{\beta}^{L_{+}^{cd}}\frac{1}{\alpha\left(\alpha+\beta\right)}+D_{\alpha}^{L_{+}^{ab}}D_{\beta}^{L_{-}^{cd}}\frac{1}{\beta\left(\alpha+\beta\right)}
\]

\end_inset

Let's check 
\begin_inset Formula 
\[
R_{0}(0,0,0,0)=\left(-\frac{\partial}{\partial\alpha}\right)\left(-\frac{\partial}{\partial\beta}\right)^{2}\frac{1}{\alpha\left(\alpha+\beta\right)}+\left(-\frac{\partial}{\partial\alpha}\right)^{2}\left(-\frac{\partial}{\partial\beta}\right)\frac{1}{\beta\left(\alpha+\beta\right)}
\]

\end_inset


\begin_inset Formula 
\[
=\left(-\frac{\partial}{\partial\alpha}\right)\frac{2}{\alpha\left(\alpha+\beta\right)^{3}}+\left(-\frac{\partial}{\partial\beta}\right)\frac{2}{\beta\left(\alpha+\beta\right)^{3}}
\]

\end_inset


\begin_inset Formula 
\[
=\frac{2}{\alpha^{2}\left(\alpha+\beta\right)^{3}}+\frac{6}{\alpha\left(\alpha+\beta\right)^{4}}+\frac{6}{\beta\left(\alpha+\beta\right)^{4}}+\frac{2}{\beta^{2}\left(\alpha+\beta\right)^{3}}
\]

\end_inset


