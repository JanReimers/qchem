\end_layout

\begin_layout Standard
\begin_inset Formula $l_{a}=l_{c}$
\end_inset

 and 
\begin_inset Formula $l_{b}=l_{d}$
\end_inset

,
 
\begin_inset Formula $\left|l_{a}-l_{b}\right|\leq k\leq l_{a}+l_{b}$
\end_inset


\begin_inset Formula 
\[
r_{k}\left(l_{a},l_{b},l_{a},l_{b}\right)=\left(-1\right)^{l_{a}+l_{b}+L}\left(2l_{a}+1\right)\left(2l_{b}+1\right)\times\left(\begin{array}{ccc}
