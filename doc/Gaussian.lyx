\end_inset


\begin_inset Formula 
\[
D_{ab}^{2}I_{ab}(n)=I_{ab}(n)\left[\left(I_{ab}^{(1)}(n)\right)^{2}+I_{ab}^{(2)}(n)\right]
\]

\end_inset


\begin_inset Formula 
\[
D_{ab}^{3}I_{ab}(n)=I_{ab}(n)I_{ab}^{(1)}(n)\left[\left(I_{ab}^{(1)}(n)\right)^{2}+I_{ab}^{(2)}(n)\right]+I_{ab}(n)\left[2I_{ab}^{(1)}(n)I_{ab}^{(2)}(n)+I_{ab}^{(3)}(n)\right]
\]

\end_inset


\begin_inset Formula 
\[
=I_{ab}(n)\left[\left(I_{ab}^{(1)}(n)\right)^{3}+3I_{ab}^{(1)}(n)I_{ab}^{(2)}(n)+I_{ab}^{(3)}(n)\right]
\]

\end_inset

For 
\begin_inset Formula $m>0$
\end_inset


\begin_inset Formula 
\[
I_{ab}^{(m+1)}(n)=D_{ab}^{m}\left(\frac{n}{2\alpha_{abcd}}+\frac{1}{\alpha_{ab}}\right)
\]

\end_inset


\begin_inset Formula 
\[
=m!\left(\frac{n}{2\alpha_{abcd}^{m+1}}+\frac{1}{\alpha_{ab}^{m+1}}\right)
\]

\end_inset

